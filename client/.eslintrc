// Use this file as a starting point for your project .eslintrc.
// Copy this file, and add rule overrides as needed.
{
    'extends': 'airbnb',
    'rules': {
        'indent': [2],
        'no-underscore-dangle': 0,
        'no-console': 0,
        'arrow-body-style': 0,
        'max-len': 0,
        'import/no-unresolved': ['error', { ignore: ['^formFramework*', '^base*', '^menu*', '^components*', '^actions*', '^stores*', '^client*', '^services*', '^config*', '^scss*'] }],
    }
}
